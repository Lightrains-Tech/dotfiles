{
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "editor.fontFamily": "Monaco, 'Courier New', monospace",
    "editor.fontSize": 15,
    "editor.suggestFontSize": 14,
    "editor.formatOnSave": true,
    "editor.minimap.maxColumn": 90,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "emmet.triggerExpansionOnTab": true,
    "editor.find.loop": false,
    "grunt.autoDetect": "off",
    "php.suggest.basic": false,
    "php.validate.enable": false,
    "terminal.integrated.fontFamily": "Monaco",
    "terminal.integrated.experimentalLinkProvider": false,
    "terminal.external.osxExec": "iterm",
    "prettier.singleQuote": true,
    "editor.wordWrap": "on",
    "editor.rulers": [80],
    "editor.wordWrapColumn": 80,
    "editor.codeLens": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.highlightActiveIndentGuide": false,
    "editor.renderIndentGuides": false,
    "editor.tabCompletion": "onlySnippets",
    "editor.formatOnPaste": true,
    "editor.minimap.enabled": false,
    "files.insertFinalNewline": true,
    "workbench.preferredHighContrastColorTheme": "Solarized Dark",
    "workbench.tips.enabled": false,
    "zenMode.hideStatusBar": false,
    "window.titleBarStyle": "native",
    "explorer.incrementalNaming": "smart",
    "debug.console.fontSize": 14,
    "extensions.autoUpdate": false,
    "extensions.ignoreRecommendations": true,
    "update.mode": "manual",
    "update.showReleaseNotes": false,
    "tailwindCSS.colorDecorators": "off",
    "tailwindCSS.emmetCompletions": true,
    "workbench.startupEditor": "newUntitledFile",
    "atomKeymap.promptV3Features": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "workbench.enableExperiments": false,
    "editor.cursorBlinking": "smooth",
    "explorer.sortOrder": "type",
    "files.associations": {
        ".babelrc": "jsonc",
        ".eslintrc": "jsonc",
        ".prettierrc": "jsonc"
    },
    "explorer.confirmDragAndDrop": false,
    "window.zoomLevel": 0.6,
    "markdown.preview.fontSize": 14,
    "terminal.integrated.fontSize": 14,
    "importCost.javascriptExtensions": ["\\.jsx?$"],
    "debug.javascript.usePreview": false,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.iconTheme": "atom-icons",
    "workbench.colorTheme": "Atom One Dark",
    "editor.stickyTabStops": true
}
