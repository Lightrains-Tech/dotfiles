{
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "editor.fontFamily": "Monaco, 'Courier New', monospace",
    "editor.fontSize": 16,
    "editor.formatOnSave": true,
    "editor.minimap.maxColumn": 90,
    "editor.tabSize": 2,
    "emmet.triggerExpansionOnTab": true,
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "editor.find.loop": false,
    "grunt.autoDetect": "off",
    "php.suggest.basic": false,
    "php.validate.enable": false,
    "terminal.integrated.fontSize": 15,
    "terminal.integrated.fontFamily": "Monaco",
    "terminal.integrated.experimentalLinkProvider": false,
    "terminal.external.osxExec": "iterm",
    "workbench.colorTheme": "Atom One Dark",
    "prettier.singleQuote": true,
    "editor.wordWrap": "on",
    "editor.codeLens": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.highlightActiveIndentGuide": false,
    "editor.renderIndentGuides": false,
    "editor.tabCompletion": "onlySnippets",
    "editor.formatOnPaste": true,
    "editor.minimap.enabled": false,
    "files.insertFinalNewline": true,
    "workbench.preferredHighContrastColorTheme": "Solarized Dark",
    "workbench.tips.enabled": false,
    "zenMode.hideStatusBar": false,
    "window.titleBarStyle": "native",
    "explorer.incrementalNaming": "smart",
    "debug.console.fontSize": 14,
    "extensions.autoUpdate": false,
    "extensions.ignoreRecommendations": true,
    "update.mode": "manual",
    "update.showReleaseNotes": false,
    "tailwindCSS.colorDecorators": "off",
    "tailwindCSS.emmetCompletions": true,
    "editor.codeActionsOnSave": null,
    "workbench.startupEditor": "newUntitledFile",
    "atomKeymap.promptV3Features": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "workbench.enableExperiments": false,
    "editor.cursorBlinking": "smooth",
    "explorer.sortOrder": "type"
}
